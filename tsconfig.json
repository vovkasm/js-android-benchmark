{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "types": ["react-native"],
    "lib": [
      "es2021"
    ],
    "jsx": "react-native",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": false
  },
  "exclude": [ "node_modules" ]
}
